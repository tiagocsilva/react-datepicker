{
  "author": "HackerOne",
  "name": "react-datepicker",
  "description": "A simple and reusable datepicker component for React",
  "version": "4.2.1",
  "license": "MIT",
  "main": "dist/index.js",
  "browser": "dist/react-datepicker.min.js",
  "module": "dist/es/index.js",
  "unpkg": "dist/react-datepicker.min.js",
  "style": "dist/react-datepicker.min.css",
  "files": [
    "*.md",
    "dist",
    "lib",
    "es",
    "src/stylesheets"
  ],
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "react",
    "datepicker",
    "calendar",
    "date",
    "react-component"
  ],
  "peerDependencies": {
    "react": "^16.9.0 || ^17",
    "react-dom": "^16.9.0 || ^17"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "classnames": "^2.2.6",
    "date-fns": "^2.24.0",
    "prop-types": "^15.7.2",
    "react-onclickoutside": "^6.12.0",
    "react-popper": "^2.2.5"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production run-p build:** && run-p css:**",
    "css:prod": "sass --style compressed src/stylesheets/datepicker.scss > dist/react-datepicker.min.css",
    "css:modules:prod": "sass --style compressed src/stylesheets/datepicker-cssmodules.scss | tee dist/react-datepicker-cssmodules.min.css dist/react-datepicker-min.module.css",
    "build:js": "rollup -c"
  }
}
